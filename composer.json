{
  "name": "mimmi20/mezzio-generic-authorization-laminasviewrenderer",
  "type": "library",
  "description": "Provides a viewhelper for mezzio-generic-authorization.",
  "homepage": "https://github.com/mimmi20/mezzio-generic-authorization-laminasviewrenderer",
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.4.3 || ^8.0.0",
    "mezzio/mezzio-authentication": "^1.1.0 || ^1.2.1",
    "mezzio/mezzio-laminasviewrenderer": "^2.3.0",
    "mimmi20/mezzio-generic-authorization": "^2.0.4"
  },
  "conflict": {
    "container-interop/container-interop": "<1.2.0",
    "mezzio/mezzio-authorization": "*"
  },
  "require-dev": {
    "laminas/laminas-servicemanager": "^3.7.0",
    "mimmi20/coding-standard": "^2.4.2",
    "phpstan/extension-installer": "^1.1.0",
    "phpstan/phpstan": "^0.12.96",
    "phpstan/phpstan-deprecation-rules": "^0.12.6",
    "phpstan/phpstan-phpunit": "^0.12.22",
    "phpunit/phpunit": "^9.5.8"
  },
  "suggest": {
    "laminas/laminas-servicemanager": "to use the factories"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laminas": {
      "config-provider": "Mezzio\\GenericAuthorization\\LaminasView\\ConfigProvider"
    }
  },
  "autoload": {
    "psr-4": {
      "Mezzio\\GenericAuthorization\\LaminasView\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MezzioTest\\GenericAuthorization\\LaminasView\\": "tests/"
    }
  },
  "support": {
    "issues": "https://github.com/mimmi20/mezzio-generic-authorization-laminasviewrenderer/issues",
    "source": "https://github.com/mimmi20/mezzio-generic-authorization-laminasviewrenderer"
  }
}
